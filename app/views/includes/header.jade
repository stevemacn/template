script.
    function toggle() {
        var el = document.getElementById("navcol")
        console.log(el)
        el.classList.toggle('collapse')    
    } 

div.navbar-left.navbar-header
    ul.nav.navbar-nav(style="margin:0;")
        li(style="margin:0px;")
            a(href="/", ng-hide="hide", 
                style="float:left;") 
                    p#heading Please use Google Chrome!

button.navbar-right(icon="menu", data-toggle="collapse", 
    onclick='toggle()', 
    style="color:#428BCA; margin:10px;").navbar-toggle.collapsed
    span.icon-bar(style="background-color:#428BCA") 
    span.icon-bar(style="background-color:#428BCA") 
    span.icon-bar(style="background-color:#428BCA") 

div.navbar-right.navbar-collapse.collapse#navcol(
            ng-class="isToggled",style="height:auto;")
    ul.nav.navbar-nav(style=" margin-right:10px; max-width:300px; height:auto;")
     if (!user)
        li
            a#heading(href="/login") Log In 
     if (user)
        li 
            a#heading(href="/") #{user.uid} 
        li 
            a#heading(href="/logout") Log Out 
                                

